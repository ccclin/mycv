<div class="col-md-12">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= simple_form_for @info, url: admin_cvs_info_path, method: :put, html: { class: "form-horizontal" } do |f| %>
      <%= f.input :name %>
      <%= f.input :english_name %>
      <%= f.input :phone %>
      <%= f.input :address %>
      <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
